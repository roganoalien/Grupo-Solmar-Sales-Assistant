@font-face {
    font-family: 'carmina';
    src: url('fonts/carminam-webfont.eot');
    src: url('fonts/carminam-webfont.eot?#iefix') format('embedded-opentype'),
         url('fonts/carminam-webfont.woff2') format('woff2'),
         url('fonts/carminam-webfont.woff') format('woff'),
         url('fonts/carminam-webfont.ttf') format('truetype'),
         url('fonts/carminam-webfont.svg#carmina_md_btmedium') format('svg');
    font-weight: normal;
    font-style: normal;
}
