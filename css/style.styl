/*
-- Grid System
*/
.section
	clear both
	padding 0
	margin 0
.col
	display block
	float left
	margin 0
	padding 0
	&:first-child
		margin-left 0
.group
	zoom 1
	&:before
		content ""
		display table
	&:after
		content ""
		clear both
		display table
